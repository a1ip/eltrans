  #hotlog
    /! HotLog
    script language="javascript" type="text/javascript"
      | hotlog_js="1.0";
      | hotlog_r=""+Math.random()+"&s=391180&im=132&r="+escape(document.referrer)+"&pg="+
      | escape(window.location.href);
      | document.cookie="hotlog=1; path=/"; hotlog_r+="&c="+(document.cookie?"Y":"N");
    script language="javascript1.1" type="text/javascript"
      | hotlog_js="1.1";hotlog_r+="&j="+(navigator.javaEnabled()?"Y":"N")
    script language="javascript1.2" type="text/javascript"
      | hotlog_js="1.2";
      | hotlog_r+="&wh="+screen.width+'x'+screen.height+"&px="+
      | (((navigator.appName.substring(0,3)=="Mic"))?
      | screen.colorDepth:screen.pixelDepth)
    script language="javascript1.3" type="text/javascript"
    script language="javascript" type="text/javascript"
      | hotlog_r+="&js="+hotlog_js;
      | document.write("
      a href="http://click.hotlog.ru/?391180" target="_top"
        | <img "+
        | " src='http://hit19.hotlog.ru/cgi-bin/hotlog/count?"+
        | hotlog_r+"&' border=0 width=88 height=31 alt=HotLog><\/a>")
    noscript
      a href="http://click.hotlog.ru/?391180" target="_top"
        img alt="HotLog" border="0" height="31" src="http://hit19.hotlog.ru/cgi-bin/hotlog/count?s=391180&amp;im=132" width="88"
    /! /HotLog