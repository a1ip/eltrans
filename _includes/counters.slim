#counters_wrap.outer
  #mailru
    /! Rating@Mail.ru COUNTER
    script language="JavaScript" type="text/javascript"
      /!
        | d=document;var a='';a+=';r='+escape(d.referrer)
        | js=10//
    script language="JavaScript1.1" type="text/javascript"
      /!
        | a+=';j='+navigator.javaEnabled()
        | js=11//
    script language="JavaScript1.2" type="text/javascript"
      /!
        | s=screen;a+=';s='+s.width+'*'+s.height
        | a+=';d='+(s.colorDepth?s.colorDepth:s.pixelDepth)
        | js=12//
    script language="JavaScript1.3" type="text/javascript"
      /!
        | js=13//
    script language="JavaScript" type="text/javascript"
      /!
        | d.write('<IMG src="http://d4.ce.b0.a1.top.list.ru/counter'+
        | '?id=1107027;js='+js+a+';rand='+Math.random()+
        | '" height=1 width=1/>')
        | if(11<js)d.write('<'+'!-- ')//
    noscript
      img alt="" height="1" src="http://d4.ce.b0.a1.top.list.ru/counter?js=na;id=1107027" width="1"
    script language="JavaScript" type="text/javascript"
      /!
        | if(11<js)d.write('--'+'>')//
    /! /COUNTER
    /! Rating@Mail.ru LOGO
    a href="http://top.mail.ru/jump?from=1107027" target="_top"
      img alt="Рейтинг@Mail.ru" border="0" height="18" src="http://d4.ce.b0.a1.top.list.ru/counter?id=1107027;t=94;l=1" width="88"
    /! /LOGO
